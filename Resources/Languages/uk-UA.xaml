    <system:String x:Key="ProductKit_Almighty">Всемогутній</system:String>
    <system:String x:Key="ProductKit_Almighty_Desc">Звання: Private - Corporal&#x0a;Корпус: Dictator M0&#x0a;Гармата: Twins M0&#x0a;Фарба: Swamp</system:String>
    <system:String x:Key="ProductKit_Mosquito">Москіт</system:String>
    <system:String x:Key="ProductKit_Mosquito_Desc">Звання: Gefreiter - Master Corporal&#x0a;Корпус: Wasp M0&#x0a;Гармата: Railgun M0&#x0a;Фарба: Flora</system:String>
    <system:String x:Key="ProductKit_Cadet">Кадет</system:String>
    <system:String x:Key="ProductKit_Cadet_Desc">Звання: Gefreiter - Master Corporal&#x0a;Корпус: Hornet M0&#x0a;Гармата: Railgun M0&#x0a;Фарба: Forester</system:String>
